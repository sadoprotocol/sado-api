{
  "prettier.prettierPath": "./node_modules/prettier",
  "prettier.configPath": "./.prettierrc",
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "cSpell.words": [
    "autorestart",
    "bcrt",
    "bech32",
    "bestblock",
    "bitcoinjs",
    "blockhash",
    "blocktime",
    "btcm",
    "bullmq",
    "CHECKSIG",
    "CHECKSIGADD",
    "decompile",
    "ecpair",
    "EQUALVERIFY",
    "fastify",
    "ipfs",
    "keypool",
    "locktime",
    "orderbooks",
    "packagejson",
    "psbt",
    "rdata",
    "regtest",
    "sado",
    "sadoprotocol",
    "satoshi",
    "satoshis",
    "sats",
    "segwit",
    "tinysecp",
    "txid",
    "txinwitness",
    "unspents",
    "upserted",
    "utxo",
    "utxos",
    "varuint",
    "vout",
    "vouts",
    "vsize",
    "watchonly",
    "wpkh"
  ],
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
